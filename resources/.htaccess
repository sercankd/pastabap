<Files pasthis.db>
	Deny from all
</Files>

<Directory resources>
	Deny from all
</Directory>

<IfModule !mod_rewrite.c>
	Redirect 500
</IfModule>

<IfModule mod_rewrite.c>
	RewriteEngine on
	#RewriteBase /pasthis
	RewriteCond %{REQUEST_FILENAME} !-d
	RewriteCond %{REQUEST_FILENAME} !-f
	RewriteRule ^([a-zA-Z0-9]{6}(@raw)?)$ index.php?p=$1 [L]
</IfModule>
